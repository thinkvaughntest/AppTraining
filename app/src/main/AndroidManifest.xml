<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thinkvaughn.apptraining" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".firstLaunched"
            android:label="@string/title_activity_first_launched" >
        </activity>
        <activity
            android:name=".activityLifeCycle"
            android:label="@string/title_activity_activity_life_cycle" >
        </activity>
        <activity
            android:name=".teamsPage"
            android:label="@string/title_activity_teams_page" >
        </activity>
        <activity
            android:name=".teamTab"
            android:label="@string/title_activity_team_tab" >
        </activity>
    </application>

</manifest>
